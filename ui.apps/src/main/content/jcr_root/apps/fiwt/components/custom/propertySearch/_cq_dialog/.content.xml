<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Property Search - Makaan"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <Inputs
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Inputs"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <type
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/select"
                                                class="cq-dialog-dropdown-showhide"
                                                cq-dialog-dropdown-showhide-target=".propertySerach-options-showhide-target"
                                                fieldLabel="Inputs Authoring Type"
                                                name="./type">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <manually
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Manually"
                                                        value="manually"/>
                                                    <dynamic
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Dynamic (Page details)"
                                                        value="dynamic"/>
                                                </items>
                                            </type>
                                            <placeholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Search Input Box Place Holder"
                                                name="./placeholder"/>
                                            <inputId
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Search Input Box ID"
                                                name="./inputId"/>
                                            <manualType
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container"
                                                class="hide propertySerach-options-showhide-target"
                                                showhidetargetvalue="manually">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <well
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <propertyTypeSelect
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                composite="{Boolean}true"
                                                                fieldLabel="Property Type Select Options">
                                                                <field
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                    name="./typeOptions">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <text
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                                            fieldLabel="Text"
                                                                            name="./text"/>
                                                                        <value
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                                            fieldLabel="Value"
                                                                            name="./value"/>
                                                                    </items>
                                                                </field>
                                                            </propertyTypeSelect>
                                                            <loactionSelect
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                composite="{Boolean}true"
                                                                fieldLabel="Location Select Options">
                                                                <field
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                    name="./locationOptions">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <text
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                                            fieldLabel="Text"
                                                                            name="./text"/>
                                                                        <value
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                                            fieldLabel="Value"
                                                                            name="./value"/>
                                                                    </items>
                                                                </field>
                                                            </loactionSelect>
                                                        </items>
                                                    </well>
                                                </items>
                                            </manualType>
                                            <propertyTypeId
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Property Type Select Box ID"
                                                name="./propertyTypeId"/>
                                            <locationId
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Location Select Box ID"
                                                name="./locationId"/>
                                            <dynamicType
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container"
                                                class="hide propertySerach-options-showhide-target foundation-layout-util-vmargin"
                                                showhidetargetvalue="dynamic">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <well
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <propertiesPath
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                fieldLabel="Properties Page Path"
                                                                name="./propertiesPath"
                                                                root="/content"/>
                                                            <locationsPath
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                fieldLabel="locations Page Path"
                                                                name="./locationsPath"
                                                                root="/content"/>
                                                        </items>
                                                    </well>
                                                </items>
                                            </dynamicType>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </Inputs>
                    <button
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Button"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <btnText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Button Text"
                                                fieldLabel="Button Text"
                                                name="./btnText"/>
                                            <btnId
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Button ID"
                                                fieldLabel="Button ID"
                                                name="./btnId"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </button>
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <id
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                fieldLabel="Id"
                                                name="./id"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </general>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
