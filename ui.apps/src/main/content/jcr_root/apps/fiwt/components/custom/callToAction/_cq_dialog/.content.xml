<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Call To Action - Makaan"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <leftSection
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Left"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <type
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/select"
                                                class="cq-dialog-dropdown-showhide"
                                                cq-dialog-dropdown-showhide-target=".aboutProperty-options-showhide-target"
                                                fieldLabel="Content Type"
                                                name="./left/type">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <img
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Image"
                                                        value="image"/>
                                                    <text
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Text"
                                                        value="text"/>
                                                </items>
                                            </type>
                                            <image
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container"
                                                class="hide aboutProperty-options-showhide-target"
                                                showhidetargetvalue="image">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <well
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <img
                                                                granite:class="cmp-image__editor-file-upload"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                                class="cq-droptarget"
                                                                enableNextGenDynamicMedia="{Boolean}true"
                                                                fileNameParameter="./left/img"
                                                                fileReferenceParameter="./left/imageReference"
                                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                                name="./left/image"/>
                                                            <alt
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="alt Text"
                                                                name="./left/alt"/>
                                                        </items>
                                                    </well>
                                                </items>
                                            </image>
                                            <text
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container"
                                                class="hide aboutProperty-options-showhide-target foundation-layout-util-vmargin"
                                                showhidetargetvalue="text">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <well
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <heading
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Heading"
                                                                name="./left/heading"/>
                                                            <description
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Description"
                                                                name="./left/description"/>
                                                            <btnText
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                emptyText="Button Text"
                                                                fieldLabel="Button Text"
                                                                name="./left/btnText"/>
                                                            <btnId
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                emptyText="Button ID"
                                                                fieldLabel="Button ID"
                                                                name="./left/btnId"/>
                                                            <btnLink
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                emptyText="Button Link"
                                                                fieldLabel="Button Link"
                                                                name="./left/btnLink"/>
                                                            <secondaryBtnText
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                emptyText="Secondary Button Text"
                                                                fieldLabel="Secondary Button Text"
                                                                name="./left/SecondaryBtnText"/>
                                                            <secondaryBtnId
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                emptyText="Secondary Button ID"
                                                                fieldLabel="Secondary Button ID"
                                                                name="./left/secondaryBtnId"/>
                                                            <secondaryBtnLink
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                emptyText="Secondary Button Link"
                                                                fieldLabel="Secondary Button Link"
                                                                name="./left/secondaryBtnLink"/>
                                                        </items>
                                                    </well>
                                                </items>
                                            </text>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </leftSection>
                    <rightSection
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Right"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <type
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/select"
                                                class="cq-dialog-dropdown-showhide"
                                                cq-dialog-dropdown-showhide-target=".aboutProperty1-options-showhide-target"
                                                fieldLabel="Content Type"
                                                name="./right/type">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <img
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Image"
                                                        value="image"/>
                                                    <text
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Text"
                                                        value="text"/>
                                                </items>
                                            </type>
                                            <image
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container"
                                                class="hide aboutProperty1-options-showhide-target"
                                                showhidetargetvalue="image">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <well
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <img
                                                                granite:class="cmp-image__editor-file-upload"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                                class="cq-droptarget"
                                                                enableNextGenDynamicMedia="{Boolean}true"
                                                                fileNameParameter="./right/img"
                                                                fileReferenceParameter="./right/imageReference"
                                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                                name="./right/image"/>
                                                            <alt
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="alt Text"
                                                                name="./right/alt"/>
                                                        </items>
                                                    </well>
                                                </items>
                                            </image>
                                            <text
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container"
                                                class="hide aboutProperty1-options-showhide-target foundation-layout-util-vmargin"
                                                showhidetargetvalue="text">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <well
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <heading
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Heading"
                                                                name="./right/heading"/>
                                                            <description
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Description"
                                                                name="./right/description"/>
                                                            <btnText
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                emptyText="Button Text"
                                                                fieldLabel="Button Text"
                                                                name="./right/btnText"/>
                                                            <btnId
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                emptyText="Button ID"
                                                                fieldLabel="Button ID"
                                                                name="./right/btnId"/>
                                                            <btnLink
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                emptyText="Button Link"
                                                                fieldLabel="Button Link"
                                                                name="./right/btnLink"/>
                                                            <secondaryBtnText
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                emptyText="Secondary Button Text"
                                                                fieldLabel="Secondary Button Text"
                                                                name="./right/secondaryBtnText"/>
                                                            <secondaryBtnId
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                emptyText="Secondary Button ID"
                                                                fieldLabel="Secondary Button ID"
                                                                name="./right/secondaryBtnId"/>
                                                            <secondaryBtnLink
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                emptyText="Secondary Button Link"
                                                                fieldLabel="Secondary Button Link"
                                                                name="./right/secondaryBtnLink"/>
                                                        </items>
                                                    </well>
                                                </items>
                                            </text>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </rightSection>
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <positionsReverse
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                fieldLabel="Sec"
                                                name="./reverse"
                                                text="Sections Position Reverse"
                                                uncheckedValue="{Boolean}false"
                                                value="{Boolean}true"/>
                                            <id
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                fieldLabel="Id"
                                                name="./id"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </general>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
